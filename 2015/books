#include <iostream>
#include <vector>
#include <algorithm>

using namespace std;

int main() {
    vector<pair<int, int>> books(4);

    // Четене на входните данни
    for (int i = 0; i < 4; i++) {
        cin >> books[i].first >> books[i].second; // first - височина, second - дебелина
    }

    // Сортиране по височина (по низходящ ред) и по дебелина (по възходящ ред)
    sort(books.begin(), books.end(), [](pair<int, int>& a, pair<int, int>& b) {
        if (a.first != b.first) return a.first > b.first;  // първо сортиране по височина
        return a.second < b.second;  // ако височините са еднакви, сортиране по дебелина
    });

    // Извеждаме резултата
    for (int i = 0; i < 4; i++) {
        cout << books[i].first << " " << books[i].second << endl;
    }

    return 0;
}
