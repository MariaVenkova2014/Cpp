#include <iostream>
using namespace std;

int main() {
    int n;
    cin >> n;

    int c1 = n / 100;           // стотици
    int c2 = (n / 10) % 10;     // десетици
    int c3 = n % 10;            // единици

    // Намери най-голямо и най-малко число с тези 3 цифри
    int max1 = c1, max2 = c2, max3 = c3;

    // bubble sort за 3 цифри – сортиране по големина (от най-голямо към най-малко)
    if (max1 > max2) swap(max1, max2);
    if (max1 > max3) swap(max1, max3);
    if (max2 > max3) swap(max2, max3);

    int a = max1 * 100 + max2 * 10 + max3;  // най-голямото
    int b = max3 * 100 + max2 * 10 + max1;  // най-малкото

    cout << b - a << endl;
    return 0;
}
