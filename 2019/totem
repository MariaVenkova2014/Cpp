#include <iostream>
#include <string>
using namespace std;

int main() {
    string line1, line2, line3;
    cin >> line1 >> line2 >> line3;

    // Главен диагонал: 1-ва от 1-ви ред, 2-ра от 2-ри ред, 3-та от 3-ти ред
    string main_diag = "";
    main_diag += line1[0];
    main_diag += line2[1];
    main_diag += line3[2];

    // Вторичен диагонал: 3-та от 1-ви ред, 2-ра от 2-ри ред, 1-ва от 3-ти ред
    string sec_diag = "";
    sec_diag += line1[2];
    sec_diag += line2[1];
    sec_diag += line3[0];

    // Преобразуване в цели числа
    int main_num = stoi(main_diag);
    int sec_num = stoi(sec_diag);

    // Изчисляване на Божествената стойност
    int result = main_num * sec_num;

    cout << result << endl;
    return 0;
}
