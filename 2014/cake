#include <iostream>
using namespace std;

int main() {
    int w[3], d[3]; // тегло и диаметър
    for (int i = 0; i < 3; i++) {
        cin >> w[i] >> d[i];
    }

    // Ще държим индекси на тортите
    int order[3] = { 0, 1, 2 };

    // Сортиране на 3 торти по диаметър (низходящо),
    // ако диаметърите са равни - по тегло (низходящо)
    for (int i = 0; i < 2; i++) {
        for (int j = i + 1; j < 3; j++) {
            int di = d[order[i]];
            int dj = d[order[j]];
            int wi = w[order[i]];
            int wj = w[order[j]];

            // Ако j-тата торта има по-голям диаметър
            // или ако диаметрите са равни, но теглото на j-та е по-голямо,
            // разменяме позициите
            if ((dj > di) || (dj == di && wj > wi)) {
                int temp = order[i];
                order[i] = order[j];
                order[j] = temp;
            }
        }
    }

    // Извеждаме реда - от най-голяма (долна) към най-малка (горна)
    // но индексите са 0-базови, трябва да изведем 1-базови номера
    cout << order[0] + 1 << " " << order[1] + 1 << " " << order[2] + 1 << "\n";

    return 0;
}
