#include <iostream>
using namespace std;

int main() {
	int d, m, g; // ден, месец, година
	int n, k, x; // височина на дърво, колко изкачва през деня, колко слиза през нощта

	cin >> d >> m >> g;	
	cin >> n >> k >> x;

	int days = 0; // брояч на дните

	while (true) {
        n -= k; // изкачване през деня
        if (n <= 0) break; // стигнала е върха

        n += x; // слизане през нощта

        // преминаваме към следващия ден
        d++;

        // проверка за преминаване към нов месец
        if ((m == 1 || m == 3 || m == 5 || m == 7 || m == 8 || m == 10 || m == 12) && d > 31) {
            d = 1;
            m++;
        } else if ((m == 4 || m == 6 || m == 9 || m == 11) && d > 30) {
            d = 1;
            m++;
        } else if (m == 2 && d > 28) { // февруари винаги 28 дни, защото няма високосна година
            d = 1;
            m++;
        }

        // проверка за преминаване към нова година
        if (m > 12) {
            m = 1;
            g++;
        }
	}

	cout << d << " " << m << " " << g << endl; // извеждаме датата
}
