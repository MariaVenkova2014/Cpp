#include <iostream>
using namespace std;

int main() {
    int N;
    cin >> N;

    // Extract each digit
    int d1 = N / 100000;
    int d2 = (N / 10000) % 10;
    int d3 = (N / 1000) % 10;
    int d4 = (N / 100) % 10;
    int d5 = (N / 10) % 10;
    int d6 = N % 10;

    // Count odd digits
    int oddCount = 0;
    if (d1 % 2 != 0) oddCount++;
    if (d2 % 2 != 0) oddCount++;
    if (d3 % 2 != 0) oddCount++;
    if (d4 % 2 != 0) oddCount++;
    if (d5 % 2 != 0) oddCount++;
    if (d6 % 2 != 0) oddCount++;

    // Remove even positions: positions 2, 4, 6
    int num1 = d1 * 100 + d3 * 10 + d5;
    int num2 = d5 * 100 + d3 * 10 + d1; // reversed

    int diff = num1 - num2;
    if (diff < 0) diff = -diff; // absolute value

    // Combine first digit (oddCount) with diff
    cout << oddCount << diff << endl;

    return 0;
}
