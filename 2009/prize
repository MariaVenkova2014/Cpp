#include <iostream>
using namespace std;

int main() {
    int a, b, c, d, e, f, g;
    cin >> a >> b >> c >> d >> e >> f >> g;

    int starsCount = 0;
    bool hasTwo = false;

    // Put grades in an array for easier loop
    int grades[7] = {a, b, c, d, e, f, g};

    for (int i = 0; i < 7; i++) {
        if (grades[i] == 2) hasTwo = true;
        if (grades[i] == 6) starsCount++;
    }

    if (hasTwo) {
        cout << "No";
    } else {
        if (starsCount > 0) {
            for (int i = 0; i < starsCount; i++) cout << "*";
        } else {
            cout << "No";
        }
    }
    cout << endl;
}
