#include <iostream>
using namespace std;

int main() {
    int n;
    cin >> n;

    // Вземаме отделните цифри от шестцифрения номер
    int d1 = n / 100000;          // първа цифра
    int d2 = (n / 10000) % 10;    // втора цифра
    int d3 = (n / 1000) % 10;     // трета цифра
    int d4 = (n / 100) % 10;      // четвърта цифра
    int d5 = (n / 10) % 10;       // пета цифра
    int d6 = n % 10;              // шеста цифра

    int sum1 = d1 + d2 + d3;
    int sum2 = d4 + d5 + d6;

    bool cond1 = (sum1 == sum2);      // първо условие - суми равни
    bool cond2 = (d2 >= d4);           // второ условие - втора цифра >= четвърта

    if (cond1 && cond2) {
        cout << sum1 << "\n";          // билетът е щастлив, извеждаме сумата
    } else if (!cond1 && !cond2) {
        cout << 30 << "\n";            // нито едно условие не е изпълнено
    } else if (!cond1 && cond2) {
        cout << 31 << "\n";            // първото условие не е изпълнено, второто е
    } else {
        // cond1 е изпълнено, cond2 не е
        cout << 32 << "\n";
    }

    return 0;
}
