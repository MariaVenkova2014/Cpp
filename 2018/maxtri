#include <iostream>
#include <algorithm>
using namespace std;

int main() {
    int a, b, c;
    cin >> a >> b >> c;

    // Обхождаме всички възможни обиколки:
    int num1 = a * 100 + b * 10 + c; // по часовника: a -> b -> c
    int num2 = b * 100 + c * 10 + a; // b -> c -> a
    int num3 = c * 100 + a * 10 + b; // c -> a -> b

    int num4 = a * 100 + c * 10 + b; // обратно: a -> c -> b
    int num5 = c * 100 + b * 10 + a; // c -> b -> a
    int num6 = b * 100 + a * 10 + c; // b -> a -> c

    int maxNum = max({num1, num2, num3, num4, num5, num6});

    cout << maxNum << endl;
    return 0;
}
