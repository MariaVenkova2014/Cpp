#include <iostream> // За cin и cout
#include <string>   // За работа със стрингове
using namespace std;

int main() {
    string pass1, pass2;
    cin >> pass1 >> pass2; // Прочитаме двете пароли

    int sum1 = 0, sum2 = 0; // Суми за всяка парола

    // Пресмятаме стойността на първата парола
    for (int i = 0; i < 4; i++) {
        char c = pass1[i];

        if (c >= 'a' && c <= 'z') {
            sum1 += (c - 'a' + 1); // малка буква
        } else if (c >= 'A' && c <= 'Z') {
            sum1 += 2 * (c - 'A' + 1); // главна буква (умножено по 2)
        } else if (c >= '0' && c <= '9') {
            sum1 += (c - '0'); // цифра
        }
    }

    // Пресмятаме стойността на втората парола
    for (int i = 0; i < 4; i++) {
        char c = pass2[i];

        if (c >= 'a' && c <= 'z') {
            sum2 += (c - 'a' + 1);
        } else if (c >= 'A' && c <= 'Z') {
            sum2 += 2 * (c - 'A' + 1);
        } else if (c >= '0' && c <= '9') {
            sum2 += (c - '0');
        }
    }

    // Сравняваме стойностите
    if (sum1 > sum2) {
        cout << pass1 << " " << sum1 << endl;
    } else if (sum2 > sum1) {
        cout << pass2 << " " << sum2 << endl;
    } else {
        cout << 0 << endl;
    }

    return 0;
}
