#include <iostream>
using namespace std;

int main() {
	int number;
	cin >> number;

	int a = number / 100;
	int b = (number / 10) % 10;
	int c = number % 10;

	int res1 = a + b + c;        // + и +
	int res2 = a + b * c;        // + и *
	int res3 = a * b + c;        // * и +
	int res4 = a * b * c;        // * и *

	int max = res1; 

	if (res2 > max) max = res2;
	if (res3 > max) max = res3;
	if (res4 > max) max = res4;

	cout << max << endl;
}
